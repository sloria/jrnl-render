{
  "compilerOptions": {
    "sourceMap": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "module": "esnext",
    "jsx": "react",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "moduleResolution": "node",
    "noImplicitAny": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
