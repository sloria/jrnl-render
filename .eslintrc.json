{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:react/recommended", "plugin:jest/recommended", "prettier/react"],
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": ["error", {"ignoreRestSiblings": true}]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
